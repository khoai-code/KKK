# Supabase Configuration
# Get these from your Supabase project settings: https://supabase.com/dashboard/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Google Sheets API
# Create API key from Google Cloud Console: https://console.cloud.google.com/apis/credentials
GOOGLE_SHEETS_API_KEY=your_google_sheets_api_key
GOOGLE_SHEET_ID=your_google_sheet_id

# Note: Google OAuth Client ID and Secret should be configured in Supabase Dashboard
# NOT in .env.local - Go to: https://supabase.com/dashboard/project/YOUR_PROJECT/auth/providers

# ClickHouse Database
CLICKHOUSE_API_URL=your_clickhouse_api_url
CLICKHOUSE_AUTH_BASIC=your_clickhouse_auth_basic
CLICKHOUSE_CF_CLIENT_ID=your_clickhouse_cf_client_id
CLICKHOUSE_CF_CLIENT_SECRET=your_clickhouse_cf_client_secret
CLICKHOUSE_ACCOUNT_NAME=your_clickhouse_account_name
CLICKHOUSE_ACCOUNT_PASSWORD=your_clickhouse_account_password
CLICKHOUSE_URL=your_clickhouse_url

# OpenAI Configuration
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key
OPEN_API_URL=your_openai_api_url
AI_MODEL=gpt-4o

# Application Configuration
# IMPORTANT: For production deployment (Vercel), change this to your actual deployment URL
# Example: https://your-app-name.vercel.app
NEXT_PUBLIC_APP_URL=http://localhost:3000
